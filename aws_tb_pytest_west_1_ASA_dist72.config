Test-Hybrid_SG_MGMT(SECURITY_GROUP):
  vpc-id: Test-Hybrid_VPC_Sec
  description: My security group
  action:
    authorize-security-group-ingress:
      - protocol: tcp
        port: 22
        cidr: 0.0.0.0/0
      - protocol: tcp
        port: 23
        cidr: 0.0.0.0/0
      - protocol: tcp
        port: 80
        cidr: 0.0.0.0/0
      - protocol: icmp
        port: all
        cidr: 0.0.0.0/0
      - protocol: tcp
        port: 443
        cidr: 0.0.0.0/0
      - protocol: tcp
        port: 8305
        cidr: 0.0.0.0/0
      - protocol: udp
        port: 6081
        cidr: 0.0.0.0/0
      - protocol: udp
        port: 666
        cidr: 0.0.0.0/0
      - protocol: udp
        port: 5001
        cidr: 0.0.0.0/0
      - protocol: tcp
        port: 5001
        cidr: 0.0.0.0/0
      - protocol: tcp
        port: 23
        cidr: 0.0.0.0/0
    bind_to: Test-Hybrid_VPC_Sec
    cleanUP: True

Test-Hybrid_SG_DATA(SECURITY_GROUP):
  vpc-id: Test-Hybrid_VPC_Sec
