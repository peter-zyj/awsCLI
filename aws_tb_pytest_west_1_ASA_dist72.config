Test-Hybrid-EC2-App-JB(EC2INSTANCE):
  image-id: ami-031b673f443c2172c
  instance-type: t2.micro
  key-name: testDog
  security-group-ids: Test-Hybrid_SG_MGMT
  count: 1
  subnet-id: Test-Hybrid_SUB_MGMT
